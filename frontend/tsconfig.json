{
  "compilerOptions": {
    "target": "es5",
    "paths": {
      "test-utils":["./utils/test-utils"]
    },
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src", "custom.d.ts", "jest.setup.ts"
, "setupTests.ts", "mocks/fileMock.ts"],

}
